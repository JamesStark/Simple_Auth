               <footer><a href="https://en.wikipedia.org/wiki/A._P._J._Abdul_Kalam" style="font-size: 25px; color: mediumspringgreen;"> Dr.A.P.J.Abdul Kalam</a></footer>
             </blockquote>

           </div>



            </div>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <div class="registrationform">
            <form class="form-horizontal" action="/register" method="POST" >
                <fieldset>
                    <legend>Registration Form <i class="fa fa-pencil pull-right"></i></legend>
                    <div class="form-group" style="padding-bottom: 18px;">
                        <!-- <label for="name" class="col-lg-2 control-label">Name</label> -->
                        <div class="col-lg-10">
                            <input type="text" class="form-control" name="name" placeholder="FullName" required>
                        </div>
                    </div>

                    <div class="form-group" style="padding-bottom: 18px">
                        <!-- <label for="email" class="col-lg-2 control-label">Email</label> -->
                        <div class="col-lg-10">
                            <input type="email" class="form-control" name="email" placeholder="Email" required>
                        </div>
                    </div>

                    <div class="form-group" style="padding-bottom: 18px">
                        <!-- <label for="number" class="col-lg-2 control-label">
                            PhoneNumber</label> -->
                        <div class="col-lg-10">
                            <input type="number" class="form-control" name="phonenumber" placeholder="Phone-number" required>
                        </div>
                    </div>

                    <div class="form-group" style="padding-bottom: 18px">
                        <!-- <label for="username" class="col-lg-2 control-label">
                            Username</label> -->
                        <div class="col-lg-10">
                            <input type="text" class="form-control" name="username" placeholder="Username" required>
                        </div>
                    </div>

                    <div class="form-group" style="padding-bottom: 18px">
                        <!-- <label for="password" class="col-lg-2 control-label">
                            Password</label> -->
                        <div class="col-lg-10">
                            <input type="password" class="form-control" name="password" placeholder="Password" required>
                        </div>
                    </div>

                    <div class="form-group" style="padding-bottom: 22px">
                        <!-- <label for="password2" class="col-lg-2 control-label">
                            ConfirmPassword</label> -->
                        <div class="col-lg-10">
                            <input type="password" class="form-control" name="password2" placeholder="ConfirmPassword">
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-lg-10 col-lg-offset-2">
                            <button type="reset" class="btn btn-warning">
                                Cancel</button>
                            <button type="submit" class="btn btn-primary">
                                Submit</button>
                        </div>
                    </div>
                </fieldset>
            </form>
         </div>



         </div>
